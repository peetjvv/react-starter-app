{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "Basic starter react app",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --env development --mode=development --host 0.0.0.0",
    "clean": "rimraf build",
    "webpack-build": "webpack --env production --mode=production",
    "build": "npm run clean && npm run webpack-build"
  },
  "author": "Peet Janse van Vuuren",
  "license": "MIT",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "json-loader": "^0.5.7",
    "modernizr-loader": "^1.0.1",
    "node-sass-json-importer": "^4.3.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
