{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "Basic starter react app",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --mode=development",
    "clean": "rimraf build",
    "build": "npm run clean && webpack --mode=production",
    "deploy": "npm run build && firebase deploy"
  },
  "author": "Peet Janse van Vuuren",
  "license": "MIT",
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "css-loader": "^5.1.2",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "json-loader": "^0.5.7",
    "modernizr-loader": "^1.0.1",
    "node-sass-json-importer": "^4.3.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
